{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": [

  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "accessor-pairs": 2,
    "react/prop-types": 0,
    "react/jsx-uses-vars": [2]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
